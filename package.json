{
  "name": "jshape",
  "version": "1.0.0",
  "description": "Shape any kind of value in JavaScript",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "jonathantneal/jshape",
  "homepage": "https://github.com/jonathantneal/jshape#readme",
  "bugs": "https://github.com/jonathantneal/jshape/issues",
  "main": "index.cjs.js",
  "module": "index.es.mjs",
  "files": [
    "browser.js",
    "index.cjs.js",
    "index.cjs.js.map",
    "index.es.mjs",
    "index.es.mjs.map"
  ],
  "scripts": {
    "build": "npm run build:browser && npm run build:node",
    "build:browser": "rollup -c .rollup.browser.js --silent",
    "build:node": "rollup -c .rollup.js --silent",
    "prepublishOnly": "npm run build && npm run test",
    "pretest": "npm run build:node",
    "test": "npm run test:js && npm run test:tape",
    "test:js": "eslint index.js --cache --ignore-path .gitignore --quiet",
    "test:tape": "node .tape"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-dev": "^2.0.0",
    "pre-commit": "^1.2.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-terser": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "babel-eslint"
  },
  "keywords": [
    "js",
    "javascript",
    "shape",
    "data",
    "types",
    "booleans",
    "arrays",
    "objects",
    "numbers",
    "primatives",
    "defaults",
    "optionals",
    "props",
    "types",
    "proptypes"
  ]
}
